<?xml version="1.0" encoding="UTF-8"?>
<Installer>
	<Name>Natron</Name>
	<Version>_VERSION_</Version>
	<Title>Natron</Title>
	<Publisher>INRIA</Publisher>
	<StartMenuDir>Natron</StartMenuDir>
	<ProductUrl>http://natron.inria.fr</ProductUrl>
	<RunProgram>@TargetDir@/Natron</RunProgram>
	<UninstallerName>MaintenanceTool</UninstallerName>
	<InstallerWindowIcon>logo.png</InstallerWindowIcon>
	<Watermark>watermark.png</Watermark>
	<!--<Background>background.png</Background>-->
        <AdminTargetDir>/opt/@ProductName@-@ProductVersion@</AdminTargetDir>
	<TargetDir>@HomeDir@/@ProductName@-@ProductVersion@</TargetDir>
	<RemoteRepositories>
		<Repository>
			<Url>http://fxarena.net/natron/snapshots/</Url>
			<Enabled>1</Enabled>
		</Repository>
	</RemoteRepositories>
</Installer>
